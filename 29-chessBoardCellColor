// https://codefights.com/arcade/intro/level-6/t97bpjfrMDZH8GJhi

function chessBoardCellColor(cell1, cell2) {
    var cols={"A":1,"B":2,"C":3,"D":4,"E":5,"F":6,"G":7,"H":8};

    let total1=cols[cell1[0]]+parseInt(cell1[1]);
    let total2=cols[cell2[0]]+parseInt(cell2[1]);
 
    return total1%2==total2%2;
}
